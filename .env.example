# EV Rental System - Frontend Environment Variables (Example)
# Copy file này thành .env và điều chỉnh theo môi trường của bạn

# API Configuration
VITE_API_BASE_URL=http://localhost:5085
VITE_API_TIMEOUT=30000

# API Endpoints
VITE_API_AUTH_URL=http://localhost:5085/api/Auth
VITE_API_STATIONS_URL=http://localhost:5085/api/Stations
VITE_API_VEHICLES_URL=http://localhost:5085/api/Vehicles
VITE_API_BOOKINGS_URL=http://localhost:5085/api/Bookings
VITE_API_RENTALS_URL=http://localhost:5085/api/Rentals
VITE_API_PAYMENTS_URL=http://localhost:5085/api/Payments

# Swagger Documentation
VITE_SWAGGER_URL=http://localhost:5085/swagger

# Application Settings
VITE_APP_NAME=EV Rental System
VITE_APP_VERSION=1.0.0

